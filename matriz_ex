def ex_01(x, y):
    matriz = []
    row = []
    for i in range(x):
        matriz.append(1)
        row.append(0)


    print(matriz)
    print(row)


ex_01(3, 3)

def generate_matriz(x, y):
    matriz = []
    rows = []

    for i in range(x):
        matriz.append(0)
        rows.append(0)
    for j in range(y):
        print(matriz)
generate_matriz(5, 5)


